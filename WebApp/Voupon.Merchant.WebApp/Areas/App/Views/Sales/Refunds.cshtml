                                        }
                                        else if (item.status == 7) {
                                            template.find(".order-status").addClass("orange").html("Pending Payment");
                                        }
                                        else if (item.status == 8) {
                                            template.find(".order-status").addClass("orange").html("Pending Refund");
                                        }
